{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "@assets/*": ["assets/*"],
      "styles/*": ["styles/*"],
      "@config": ["config.ts"],
      "@components/*": ["shared/components/*"],
      "@content/*": ["content/*"],
      "@layouts/*": ["shared/layouts/*"],
      "@pages/*": ["pages/*"],
      "@styles/*": ["shared/styles/*"],
      "@utils/*": ["shared/utils/*"]
      // "@shared/*": ["shared/*"]
    }
  }
}
