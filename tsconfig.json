{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "@assets/*": ["assets/*"],
      "@config": ["config.ts"],
      "@pages/*": ["pages/*"],
      "@data/*": ["data/*"],
      "@shared/*": ["shared/*"],
      "@components/*": ["shared/components/*"],
      "@content/*": ["content/*"],
      "@layouts/*": ["shared/layouts/*"],
      "@styles/*": ["shared/styles/*"],
      "@utils/*": ["shared/utils/*"],
      "@icons/*": ["shared/icons/*"]
    }
  }
}
